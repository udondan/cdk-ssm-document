{
  "name": "cdk-ssm-document",
  "description": "CDK Construct for managing SSM Documents",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/cdk-ssm-document",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/cdk-ssm-document.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/cdk-ssm-document/issues"
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "aws-cdk-construct",
    "ssm",
    "aws-ssm",
    "ssm-document",
    "aws-ssm-document",
    "cloudformation",
    "cloudformation-custom-resource"
  ],
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "prepackage": "jsii && lambda/build"
  },
  "jsii": {
    "excludeTypescript": [
      "lambda"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-ssm-document",
        "module": "cdk_ssm_document"
      }
    }
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "17.0.15",
    "aws-cdk-lib": "^2.0.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1069.0",
    "constructs": "^10.0.0",
    "jsii": "^1.52.1",
    "jsii-pacmak": "^1.52.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "cdk-iam-floyd": "^0.300.0",
    "aws-cloudformation-custom-resource": "^2.0.1"
  },
  "dependencies": {
    "js-yaml": "^3.14.1"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "cdk-iam-floyd": "^0.300.0",
    "constructs": "^10.0.0"
  },
  "bundleDependencies": [
    "js-yaml"
  ]
}
