{
  "name": "cdk-ssm-document",
  "description": "CDK Construct for managing SSM Documents",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/cdk-ssm-document",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/cdk-ssm-document.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/cdk-ssm-document/issues"
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "aws-cdk-construct",
    "ssm",
    "aws-ssm",
    "ssm-document",
    "aws-ssm-document",
    "cloudformation",
    "cloudformation-custom-resource"
  ],
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "prepackage": "jsii && lambda/build"
  },
  "jsii": {
    "excludeTypescript": [
      "lambda"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-ssm-document",
        "module": "cdk_ssm_document"
      },
      "dotnet": {
        "namespace": "CDK.SSM.Document",
        "packageId": "CDK.SSM.Document"
      }
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.62.0",
    "@types/aws-lambda": "^8.10.62",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "14.6.4",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.747.0",
    "jsii": "^1.12.0",
    "jsii-pacmak": "^1.12.0",
    "ts-node": "^9.0.0",
    "typescript": "~4.0.2"
  },
  "dependencies": {
    "@aws-cdk/aws-cloudformation": "^1.47.0",
    "@aws-cdk/aws-iam": "^1.47.0",
    "@aws-cdk/aws-lambda": "^1.47.0",
    "@aws-cdk/core": "^1.47.0",
    "cdk-iam-floyd": "^0.62.6"
  },
  "peerDependencies": {
    "@aws-cdk/aws-cloudformation": "^1.47.0",
    "@aws-cdk/aws-iam": "^1.47.0",
    "@aws-cdk/aws-lambda": "^1.47.0",
    "@aws-cdk/core": "^1.47.0",
    "cdk-iam-floyd": "^0.62.6",
    "constructs": "^3.0.4"
  }
}
